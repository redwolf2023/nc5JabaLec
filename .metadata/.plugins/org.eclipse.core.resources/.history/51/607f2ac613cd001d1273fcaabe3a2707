package chap17_generic.clazz;

public class BoardFile<T> {
	//T == FreeBoard는 boardNo == fBoardNo;
	//T == NoticeBoad는 boardNo == nBoardNo;
	private int boardNo;
	private int type;
	private String boardFileNm;
	
	private T t; //FreeBoard fBoard

	public int getBoardNo() {
		return boardNo;
	}

	public void setBoardNo(int boardNo) {
		this.boardNo = boardNo;
	}

	public int getTyte() {
		return type;
	}

	public void setTyte(int tyte) {
		this.type = type;
	}

	public String getBoardFileNm() {
		return boardFileNm;
	}

	public void setBoardFileNm(String boardFileNm) {
		this.boardFileNm = boardFileNm;
	}

	public T getT() {
		return t;
	}

	public void setT(T t) {
		this.t = t;
	}

	@Override
	public String toString() {
		return "BoradFile [boardNo=" + boardNo + ", tyte=" + tyte + ", boardFileNm=" + boardFileNm + ", t=" + t + "]";
	}
	
	

}
