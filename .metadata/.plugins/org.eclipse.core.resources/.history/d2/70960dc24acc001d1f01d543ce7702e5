package homework6.business;

public class FullTimeMan extends BusinessMan {
	
	int bouns;
	

	public FullTimeMan(int id, String name, int pay,int bonus) {
		super(id, name, pay);
		this.bouns = bonus;
	}

	@Override
	public double getMonthPay() {
		
		return (double)getPay() / 12 + bouns / 12;
	}

}
