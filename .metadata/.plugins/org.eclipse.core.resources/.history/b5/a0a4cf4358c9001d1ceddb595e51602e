package homework8;

public class Basic {

	public static void main(String[] args) {

		StringBuffer sb = new StringBuffer();
		StringBuffer sb1 = new StringBuffer(30);
		String str1 = "abc";
		String str2 = "def";
		String str3 = "ghi";
		sb.append(str1);
		sb.append(str2);
		sb.append(str3);
		System.out.println(sb);
		
		String str4 = "hello";
		sb1.append(str4);
		sb1.length();
		System.out.println(sb1);
				
		
	}

}
